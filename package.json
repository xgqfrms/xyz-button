{
    "name": "xyz-button",
    "version": "1.0.6",
    "description": "An Vue UI Button Component!",
    "private": false,
    "author": "xgqfrms",
    "license": "MIT",
    "keywords": [
        "vue",
        "button",
        "ui",
        "components",
        "xyz-button"
    ],
    "files": [
        "dist/*"
    ],
    "main": "./dist/index.min.js",
    "scripts": {
        "start": "vue serve ./src/xyz-button.vue",
        "build": "bili --format cjs-min --no-map --plugin.vue.css true",
        "build-no-map": "bili --format cjs-min --no-map --plugin.vue.css true",
        "bili-all": "bili \"src/index.js\" --format cjs --plugin.vue.css true",
        "bili-default": "bili",
        "bili-string": "bili src/index.js --format cjs --plugin.vue.css true",
        "old-build": "bili --name index --plugin vue --vue.css false",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "bin": {
        "dev": "dev",
        "app": "app"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xgqfrms/xyz-button.git"
    },
    "bugs": {
        "url": "https://github.com/xgqfrms/xyz-button/issues"
    },
    "homepage": "https://github.com/xgqfrms/xyz-button#readme",
    "devDependencies": {
        "bili": "^4.8.0",
        "rollup-plugin-vue": "^5.0.0",
        "vue-template-compiler": "^2.6.10"
    }
}
